[fedberry-unstable]
name=FedBerry Unstable $releasever
failovermethod=priority
mirrorlist=https://fedberry.github.io/mirrorlist_unstable_$releasever
enabled=0
metadata_expire=6h
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedberry-$releasever-primary
priority=50
